    "next": "14.0.4",
    "openai": "^6.6.0",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.34.4",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
