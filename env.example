# API Configuration
NEXT_PUBLIC_LV_API_URL=https://api.landing-ai.com/v1
NEXT_PUBLIC_LV_API_KEY=your_api_key_here
NEXT_PUBLIC_MODEL_SLUG=your_model_slug_here
NEXT_PUBLIC_STORAGE_BUCKET=your_storage_bucket_here

# Model Discovery APIs (SERVER-SIDE ONLY - Never exposed to client)
# Get your Roboflow API key from: https://app.roboflow.com/settings/api
# IMPORTANT: Replace with your actual Roboflow API key
ROBOFLOW_API_KEY=your_actual_roboflow_api_key_here

# Hugging Face API Token (SERVER-SIDE ONLY - Required for model search and inference)
# Get token from: https://huggingface.co/settings/tokens
# Scopes needed: read (for search), inference (for running models)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# OpenAI API Key (SERVER-SIDE ONLY - Required for query refinement)
# Get key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini API Key (SERVER-SIDE ONLY - Required for Computer Use browsing Roboflow Universe)
# Get key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# MongoDB (Optional - for analytics)
MONGODB_URI=mongodb+srv://mobile-vision:<db_password>@lv-sandbox.awbjpib.mongodb.net/?retryWrites=true&w=majority&appName=lv-sandbox

# App Configuration
NEXT_PUBLIC_APP_NAME=Mobile Vision App Starter
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_DEFAULT_TASK=multi-type
